<includes>
	<default type="image">
		<posx>0</posx>
		<posy>0</posy>
		<texture background="true"/>
	</default>
	<default type="multiimage">
		<posx>0</posx>
		<posy>0</posy>
		<imagepath background="true"/>
	</default>
	<default type="label">
		<posx>0</posx>
		<posy>0</posy>
		<label>-</label>
		<font>font18</font>
		<textcolor>white</textcolor>
		<disabledcolor>disabled</disabledcolor>
		<selectedcolor>blue</selectedcolor>
	</default>
	<default type="fadelabel">
		<posx>0</posx>
		<posy>0</posy>
		<font>font18</font>
		<textcolor>white</textcolor>
		<disabledcolor>disabled</disabledcolor>
		<selectedcolor>blue</selectedcolor>
	</default>
	<default type="button">
		<posx>0</posx>
		<posy>0</posy>
		<width>150</width>
		<height>33</height>
		<texturefocus>grey.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<font>font18</font>
		<label>-</label>
		<textcolor>white</textcolor>
		<disabledcolor>disabled</disabledcolor>
		<textoffsetx>17</textoffsetx>
		<aligny>center</aligny>
		<pulseonselect>no</pulseonselect>
	</default>
	<default type="togglebutton">
		<posx>0</posx>
		<posy>0</posy>
		<width>150</width>
		<height>33</height>
		<texturefocus>grey.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<alttexturefocus>grey.png</alttexturefocus>
		<alttexturenofocus>-</alttexturenofocus>
		<font>font18</font>
		<label>-</label>
		<textcolor>white</textcolor>
		<disabledcolor>disabled</disabledcolor>
		<textoffsetx>17</textoffsetx>
		<aligny>center</aligny>
		<pulseonselect>no</pulseonselect>
	</default>
	<default type="textbox">
		<posx>0</posx>
		<posy>0</posy>
		<width>300</width>
		<height>200</height>
		<textcolor>white</textcolor>
		<font>font18</font>
		<align>justify</align>
		<autoscroll delay="6000" time="2000" repeat="10000">Skin.HasSetting(AutoScroll)</autoscroll>
	</default>
	<default type="progress">
		<posx>0</posx>
		<posy>0</posy>
		<texturebg border="7">osd/osd_video_indicator_background.png</texturebg>
		<lefttexture>-</lefttexture>
		<midtexture border="7">osd/osd_time_indicator.png</midtexture>
		<righttexture>-</righttexture>
	</default>
	<default type="spincontrol">
		<posx>0</posx>
		<posy>0</posy>
		<textureup>icons/scroll_up.png</textureup>
		<texturedown>icons/scroll_down.png</texturedown>
		<textureupfocus>icons/settings_arrow_button_up.png</textureupfocus>
		<texturedownfocus>icons/settings_arrow_button_down.png</texturedownfocus>
		<align>right</align>
		<width>20</width>
		<height>20</height>
		<font>font18</font>
		<reverse>yes</reverse>
		<textcolor>white</textcolor>
		<disabledcolor>disabled</disabledcolor>
		<subtype>text</subtype>
		<textoffsetx>-5</textoffsetx>
		<textoffsety>0</textoffsety>
		<aligny>-</aligny>
		<pulseonselect>no</pulseonselect>
	</default>
	<default type="spincontrolex">
		<posx>0</posx>
		<posy>0</posy>
		<width>300</width>
		<height>42</height>
		<spinwidth>20</spinwidth>
		<spinheight>20</spinheight>
		<texturefocus>grey.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<textureup>icons/scroll_up.png</textureup>
		<texturedown>icons/scroll_down.png</texturedown>
		<textureupfocus>icons/settings_arrow_button_up.png</textureupfocus>
		<texturedownfocus>icons/settings_arrow_button_down.png</texturedownfocus>
		<font>font18</font>
		<textcolor>white</textcolor>
		<disabledcolor>disabled</disabledcolor>
		<textoffsetx>7</textoffsetx>
		<aligny>center</aligny>
		<reverse>yes</reverse>
		<pulseonselect>no</pulseonselect>
	</default>
	<default type="radiobutton">
		<posx>300</posx>
		<posy>200</posy>
		<width>230</width>
		<height>42</height>
		<textureradiofocus>icons/settings_radio_button_selected.png</textureradiofocus>
		<textureradionofocus>icons/settings_radio_button.png</textureradionofocus>
		<texturefocus>grey.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<label>-</label>
		<font>font18</font>
		<textcolor>white</textcolor>
		<disabledcolor>disabled</disabledcolor>
		<textoffsetx>17</textoffsetx>
		<aligny>center</aligny>
		<pulseonselect>no</pulseonselect>
	</default>
	<default type="mover">
		<posx>60</posx>
		<posy>192</posy>
		<texturefocus>buttons/focus.png</texturefocus>
		<texturenofocus>buttons/nofocus.png</texturenofocus>
	</default>
	<default type="resize">
		<posx>60</posx>
		<posy>192</posy>
		<texturefocus>buttons/focus.png</texturefocus>
		<texturenofocus>buttons/nofocus.png</texturenofocus>
	</default>
	<default type="sliderex">
		<height>44</height>
		<texturefocus>views/line_frame_device_white.png</texturefocus>
		<texturenofocus>views/line_frame_device_black.png</texturenofocus>
		<sliderwidth>150</sliderwidth>
		<sliderheight>20</sliderheight>
		<texturesliderbar>osd/osd_slider_bg.png</texturesliderbar>
		<textureslidernib border="1">osd/osd_slider_nibNF.png</textureslidernib>
		<textureslidernibfocus border="1">osd/osd_slider_nib.png</textureslidernibfocus>
		<pulseonselect>no</pulseonselect>
		<font>font21</font>
		<textcolor>white</textcolor>
		<focusedcolor>black</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
		<textoffsetx>20</textoffsetx>
		<aligny>center</aligny>
	</default>
</includes>
